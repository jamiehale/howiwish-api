{
  "name": "howiwish-server",
  "version": "0.0.0",
  "main": "dist",
  "private": true,
  "scripts": {
    "build": "babel src -s -D -d dist",
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "lint": "eslint src",
    "prestart": "yarn build",
    "start": "node dist"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.7.0",
    "nodemon": "^1.18.4"
  }
}
